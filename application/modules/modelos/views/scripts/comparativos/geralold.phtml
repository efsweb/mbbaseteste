function ajustHeightTable(){
    var ua = window.navigator.userAgent;
    var msie = ua.indexOf("MSIE ");
    var chave = 0;
    $('.active').each(function(){
        $(this).find('table').each(function(){
            var mbtbid = '#' + $(this).attr('id');
            var cptbid = mbtbid.replace('mb','cp');
            $(mbtbid).removeAttr('style');
            $(cptbid).removeAttr('style');
            var mbheight = $(mbtbid).outerHeight();
            var cpheight = $(cptbid).outerHeight();
            if(mbheight > cpheight){
                $(cptbid).outerHeight(mbheight + 11);
            }else{
                if(msie > 0 || !!navigator.userAgent.match(/Trident.*rv\:11\./)){
                    if(mbheight != cpheight){
                        if(chave == 0){
                            chave = 1;
                            $(mbtbid).outerHeight(111);
                        }else{
                            h = cpheight - 1;
                            console.log(h);
                            $(mbtbid).outerHeight(h);
                        }
                    }
                    //console.log('MB ' + $(mbtbid).outerHeight() + '; CP ' + cpheight);
                }else{
                    $(mbtbid).outerHeight(cpheight + 11);
                }
            }
        });
    });
}

