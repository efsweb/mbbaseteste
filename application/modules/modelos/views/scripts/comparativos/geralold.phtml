<script type="text/javascript">
$(document).on('pagehide', 'div', function (event, ui) { 
    var page = jQuery(event.target);
    if (page.attr('data-cache') == 'never') { 
        page.remove();
    };
});
$('#mnulateralesquerdo').children('div:last').prev().attr('data-collapsed', 'false');
$('#mod_cp_geral').children('a').css("background","url('/images/arrow-menu-left.png')no-repeat right");

    function addline(tipo,cp){
        var mbclass = '.' + tipo + '-mb';
        var cpclass = '.' + tipo + '-conc';
        var ids = [];
        $('.carousel-inner').find(mbclass).each(function(){
            var sp = $(this).attr('id');
            var pt = sp.split('-');
            ids.push(pt[pt.length-1]);
        });
        //ids.indexOf(cp); para adicionar linha somente ao concorrente ativo
        for(i=0;i<ids.length;i++){
            var dvidmb = '#mb-' + tipo + '-' + ids[i];
            var dvidcp = '#' + ids[i] + '-' + tipo;
            var linenm = 0;
            var tblidc = $(dvidmb).children('table:last').attr('id');
            if(typeof tblidc !== 'undefined'){
                linenm = parseInt(tblidc.split('_')[3]) + 1;
            }
            var mbtblid = 'mb_' + tipo + '_' + ids[i] + '_' + linenm;
            var cptblid = 'cp_' + tipo + '_' + ids[i] + '_' + linenm;
            var mbcol01 = mbtblid + '_1';
            var mbcol02 = mbtblid + '_2';
            var cpcol01 = cptblid + '_1';
            var cpcol02 = cptblid + '_2';

            mbtbl  = '<table id="' + mbtblid + '" width="100%" border="0" cellspacing="4" cellpadding="0" class="classificacao">';
            mbtbl += '<tr><td class="item-class tamanho-fixo">';
            mbtbl += '<a id="' + mbcol01 + '" onclick="opensearch(this);" href="#SelectItem" data-rel="popup" data-position-to="window" data-transition="pop" class="ui-btn">';
            mbtbl += '<span class="icon-plus"></span>Item</a>';
            mbtbl += '</td><td class="item-class">';
            mbtbl += '<a id="' + mbcol02 + '" onclick="opensearch(this);" href="#SelectItem" data-rel="popup" data-position-to="window" data-transition="pop" class="ui-btn">';
            mbtbl += '<span class="icon-plus"></span>Descritivo MB</a>';
            mbtbl += '</td></tr></table>';

            cptbl  = '<table id="' + cptblid + '" width="100%" border="0" cellspacing="4" cellpadding="0" class="classificacao">';
            cptbl += '<tr><td class="item-class tamanho-fixo-conc">';
            cptbl += '<a id="' + cpcol01 + '" onclick="opensearch(this);" href="#SelectItem" data-rel="popup" data-position-to="window" data-transition="pop" class="ui-btn">';
            cptbl += '<span class="icon-plus"></span>Descritivo Concorrente</a>';
            cptbl += '</td><td class="item-class">';
            cptbl += '<a id="' + cpcol02 + '" onclick="opensearch(this);" href="#SelectItem" data-rel="popup" data-position-to="window" data-transition="pop" class="ui-btn">';
            cptbl += '<span class="icon-plus"></span>Argumento de Vendas</a>';
            cptbl += '</td>';
            cptbl += '<td class="icon-class icon-enable">';
            cptbl += '<a onclick="excludeme(this);" href="#" data-toggle="tooltip" title="Excluir item" class="icon-cancel-2 ui-link"></a>';
            cptbl += '</td>';
            cptbl += '</tr></table>';

            $(dvidmb).append(mbtbl);
            $(dvidcp).append(cptbl);
            ajustHeightTable();
        }
    }
    function changeme(obj){
        var img = "/images/comparativos/" + $(obj).attr('data-img');
        var mod = $(obj).attr('data-modelo');
        var id  = '#' + $(obj).attr('data-modelo-id');
        $(obj).closest('ul').prev().prev().children('img').attr('src',img);
        $(obj).closest('ul').prev().children('span').html(mod);
        setTimeout(function(){ajustHeightTable();},300); 
    }
    function excludeme(obj){
        var tblcp = '#' + $(obj).closest('table').attr('id');
